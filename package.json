{
  "name": "svelte-docster",
  "version": "1.2.0",
  "description": "Generate metadata about your Svelte files from jsdoc.",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "scripts": {
    "build": "rollup -c",
    "test": "node -r esm test/index.js",
    "examples": "npm run build && node examples/gen.js",
    "test:update": "npm run build && node ./test/fixtures.js",
    "prepublishOnly": "npm run build"
  },
  "homepage": "https://github.com/TheComputerM/svelte-docster#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TheComputerM/svelte-docster.git"
  },
  "bugs": {
    "url": "https://github.com/TheComputerM/svelte-docster/issues"
  },
  "keywords": [
    "svelte",
    "jsdoc",
    "doc"
  ],
  "author": "TheComputerM",
  "license": "MIT",
  "dependencies": {
    "comment-parser": "^1.1.5"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.2.1",
    "astray": "^1.1.1",
    "astring": "^1.7.4",
    "esm": "^3.2.25",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "tiny-glob": "^0.2.8",
    "uvu": "^0.5.1"
  },
  "peerDependencies": {
    "svelte": "^3.38.2"
  },
  "files": [
    "dist"
  ]
}
