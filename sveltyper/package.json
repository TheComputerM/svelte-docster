  },
  "bugs": {
    "url": "https://github.com/TheComputerM/svelte-docster/issues"
  },
  "dependencies": {
    "svelte-docster": "workspace:^2.0.1"
  },
  "devDependencies": {
    "astray": "^1.1.1",
    "jiti": "^1.10.1",
    "meriyah": "^4.1.5",
    "microbundle": "^0.13.3",
    "svelte": "^3.38.3",
